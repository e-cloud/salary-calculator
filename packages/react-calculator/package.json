    "@types/node": "^17.0.23",
    "@types/react": "^17.0.44",
    "@typescript-eslint/eslint-plugin": "^5.18.0",
    "@typescript-eslint/parser": "^5.18.0",
    "autoprefixer": "^10.4.4",
    "cross-env": "^7.0.3",
    "cssnano": "^5.1.7",
    "eslint": "^8.13.0",
